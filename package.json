{
	"name": "niyom_app_backend",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"start": "yarn run build && cross-env NODE_ENV=production node dist/server.js",
		"dev": "nodemon",
		"build": "swc src -d dist --source-maps --copy-files",
		"build:tsc": "tsc && tsc-alias",
		"test": "jest --forceExit --detectOpenHandles",
		"lint": "eslint --ignore-path .gitignore --ext .ts src/",
		"lint:fix": "npm run lint -- --fix",
		"prisma:db": "npx prisma studio",
		"prisma:init": "prisma init",
		"prisma:migrate": "prisma migrate dev --preview-feature",
		"prisma:generate": "prisma generate",
		"prisma:deploy": "prisma migrate deploy",
		"prisma:reset": "prisma migrate reset --force",
		"deploy:prod": "npm run build && pm2 start ecosystem.config.js --only prod",
		"deploy:dev": "pm2 start ecosystem.config.js --only dev",
		"check:script": "npx ts-node src/scripts/checkProvider.ts"
	},
	"dependencies": {
		"@prisma/client": "^5.15.1",
		"@types/express": "^4.17.21",
		"bcryptjs": "^2.4.3",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^4.19.2",
		"express-rate-limit": "^7.3.0",
		"joi": "^17.13.1",
		"jsonwebtoken": "^9.0.2",
		"morgan": "^1.10.0",
		"multer": "^1.4.5-lts.1",
		"tslib": "^2.6.3",
		"uuid": "^9.0.1"
	},
	"devDependencies": {
		"@swc/cli": "^0.3.12",
		"@swc/core": "^1.5.25",
		"@types/bcryptjs": "^2.4.6",
		"@types/cors": "^2.8.17",
		"@types/jsonwebtoken": "^9.0.6",
		"@types/morgan": "^1.9.9",
		"@types/node": "^20.12.13",
		"cross-env": "^7.0.3",
		"nodemon": "^3.1.2",
		"prisma": "^5.15.1",
		"ts-node": "^10.9.2",
		"ts-node-dev": "^2.0.0",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.4.5"
	}
}
